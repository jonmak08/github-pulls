{
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "scripts": {
    "test": "exit 0",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./app",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./app",
    "package": "cross-env NODE_ENV=production node --harmony build.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && install-app-deps",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "release": "npm run package-all -- --release",
    "dist": "build"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "cli-spinner": "^0.2.5",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "devtron": "^1.0.0",
    "electron-builder": "^5.1.0",
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "~1.0.1",
    "electron-rebuild": "^1.1.3",
    "electron-release": "^2.2.0",
    "electron-winstaller": "^2.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.0",
    "file-loader": "^0.8.5",
    "github": "^1.1.2",
    "got": "^6.3.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "node-bourbon": "^4.2.8",
    "node-sass": "^3.7.0",
    "publish-release": "^1.3.1",
    "react": "^0.14.8",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.4.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "yargs": "^4.7.1"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "2.x || 3.x"
  }
}